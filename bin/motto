#!/usr/bin/env bash

declare Yellow='\033[1;33m'
declare NoColor='\033[0m'

clear
echo -e "${Yellow}>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>"
echo -e ">>> 马拉松是一项艰苦的运动，对我来说更像一种修行。-- 基普乔格 <<<"
echo -e ">>>                                                           <<<"
echo -e ">>> 胜人者有力，自胜者强。-- 老子                             <<<"
echo -e ">>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>${NoColor}"
